{
    "usesTypeScript": true,
    "usesCssModule": true,
    "cssPreprocessor": "scss",
    "testLibrary": "Testing Library",
    "component": {
        "default": {
            "customTemplates": {
                "component": "templates/default/TemplateName.tsx",
                "style": "templates/default/TemplateName.module.scss",
                "props": "templates/default/TemplateName.props.ts",
                "index": "templates/default/index.ts"
            },
            "path": "src/components",
            "withStyle": true,
            "withTest": true,
            "withStory": false,
            "withLazy": false,
            "withProps": true,
            "withIndex": true
        },
        "page": {
            "customTemplates": {
                "component": "templates/default/TemplateName.tsx",
                "style": "templates/default/TemplateName.module.scss",
                "props": "templates/default/TemplateName.props.ts",
                "index": "templates/default/index.ts"
            },
            "path": "src/pages",
            "withStyle": true,
            "withTest": true,
            "withStory": false,
            "withLazy": false,
            "withProps": true,
            "withIndex": true
        }
    }
}
